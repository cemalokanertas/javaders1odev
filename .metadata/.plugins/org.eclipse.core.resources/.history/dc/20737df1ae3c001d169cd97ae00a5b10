package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) {

		int number = 25;
		int remainder = number % 2;
		System.out.println(remainder);
		if(remainder==0) {
			System.out.println("Sayı asal değil.");
		}
		else {
			System.out.println("Sayı asaldır.");
		}
		
	}

}
